---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ get_url() }}</loc>
        <lastmod>{{ none|date('%Y-%m-%dT%H:%M:%S%Z+00:00') }}</lastmod>
    </url>
    <url>
        <loc>{{ get_url('about') }}</loc>
        <lastmod>{{ none|date('%Y-%m-%dT%H:%M:%S%Z+00:00') }}</lastmod>
    </url>
    <url>
        <loc>{{ get_url('blog') }}</loc>
        <lastmod>{{ none|date('%Y-%m-%dT%H:%M:%S%Z+00:00') }}</lastmod>
    </url>
    {% for post in posts %}
    <url>
        <loc>{{ get_url(post.url) }}</loc>
        <lastmod>{{ post.timestamp|date('%Y-%m-%dT%H:%M:%S%Z+00:00') }}</lastmod>
    </url>
    {% endfor %}
    {% for name, tag in tags %}
    <url>
        <loc>{{ get_url(tag.url) }}</loc>
        <lastmod>{{ none|date('%Y-%m-%dT%H:%M:%S%Z+00:00') }}</lastmod>
    </url>
    {% endfor %}
</urlset>